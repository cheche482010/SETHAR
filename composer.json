    "autoload": {
       "classmap": [
            "app/",
            "controlador/",
            "controlador/propiedades/",
            "modelo/",
            "modelo/propiedades/",
            "componentes/",
            "componentes/validacion/"
        ],
        "psr-4": {
            "App\\": "app/",
            "Controlador\\": "controlador/",
            "Validacion\\": "controlador/backend/",
            "Modulo\\": "modelo/",
