            "Traits\\": "traits/"
        },
        "files": ["app/Base_Datos.php", "app/App.php", "app/CRUD.php", "app/Modelo.php", "app/Vista.php", "app/Controlador.php"]
    },
    "autoload-dev": {
        "classmap": [
            "app/",
            "controlador/",
            "controlador/propiedades/",
            "modelo/",
            "modelo/propiedades/",
            "componentes/",
            "componentes/validacion/"
        ],
        "psr-4": {
            "App\\": "app/",
            "Controlador\\": "controlador/",
            "Modelo\\": "modelo/",
            "Traits\\": "componentes/traits/",
            "Entidad\\": "modelo/entidad/"
        }
    },
    "require": {
        "php": ">=5.3.3 <=8.1.1",
        "doctrine/dbal": "^3.3",
        "doctrine/orm": "^2.14",
        "doctrine/cache": "^1.11",
        "doctrine/annotations": "^1.0"
    },
    "config": {
        "optimize-autoloader": true
    }
}